    <connection id="182a7808-bb1f-4da2-b3d4-32432ab6fd55">
      <console name="Connection" type="STANDARD" schema="sample-database" session="Main"><![CDATA[CREATE TABLE students (
  id INTEGER PRIMARY KEY,
  first_name TEXT NOT NULL,
  last_name TEXT NOT NULL
);

CREATE TABLE teachers (
  id INTEGER PRIMARY KEY,
  first_name TEXT NOT NULL,
  last_name TEXT NOT NULL
);

-- Insert sample data into the students table
INSERT INTO students (id, first_name, last_name) VALUES
(1, 'Alice', 'Adams'),
(2, 'Bob', 'Baker'),
(3, 'Carol', 'Carter'),
(4, 'David', 'Diaz'),
(5, 'Eva', 'Evans');

-- Insert sample data into the teachers table
INSERT INTO teachers (id, first_name, last_name) VALUES
(1, 'Frank', 'Fisher'),
(2, 'Grace', 'Griffin'),
(3, 'Henry', 'Harrison'),
(4, 'Irene', 'Ingram'),
(5, 'Jack', 'Andrey');

--1-topshiriq:

SELECT first_name || ' ' || last_name AS full_name
FROM students
WHERE first_name LIKE 'A%' OR first_name LIKE 'E%' OR first_name LIKE 'I%' OR first_name LIKE 'O%' OR first_name LIKE 'U%'
UNION
SELECT first_name || ' ' || last_name AS full_name
FROM teachers
WHERE first_name LIKE 'A%' OR first_name LIKE 'E%' OR first_name LIKE 'I%' OR first_name LIKE 'O%' OR first_name LIKE 'U%';

--2-topshiriq: 

SELECT *, first_name || ' ' || last_name AS full_name FROM students 
UNION
SELECT *, first_name || ' ' || last_name AS full_name FROM teachers 
ORDER BY length(last_name) last_name;

--3-topshiriq: 

SELECT first_name || ' ' || last_name AS full_name
FROM students
UNION
SELECT first_name || ' ' || last_name AS full_name
FROM teachers;

--4-topshiriq: 

SELECT first_name || ' ' || last_name AS full_name, id
FROM students
UNION
SELECT first_name || ' ' || last_name AS full_name, id
FROM teachers ORDER BY id DESC LIMIT 3;]]></console>
    </connection>
    <connection id="e26545bd-9650-4612-917a-d3da5cc44077">
      <console name="Mysql Databases" type="STANDARD" schema="unions" session="Main"><![CDATA[--join1 vazifa
select locations.*, countries.country_name from locations 
inner join countries on locations.country_id = countries.country_id;

--join2 vazifa
select departments.*, locations.city, locations.state_province, locations.street_address 
from departments inner join locations on departments.location_id = locations.location_id order by department_id;

--join3 vazifa
select locations.*, countries.country_name from locations 
inner join countries on locations.country_id = countries.country_id
where countries.region_id= 1;

--join3 vazifada region_id Europe ga teng bo'lishi kerak edi 
--region jadvalidan region_name ustunida Europe 1 da turgani uchun man 1 ni o'zini kiritdim

--join4 vazifa
select departments.*, locations.city, locations.state_province, locations.street_address 
from departments inner join locations on departments.location_id = locations.location_id 
where locations.country_id='US' order by department_id;

--join5 vazifa
select employees.first_name, employees.last_name, employees.phone_number, departments.department_name  
from employees inner join departments on employees.department_id = departments.department_id;

--join6 vazifa 
select xodim.first_name, xodim.last_name, xodim.phone_number, concat(manager.first_name,  ' ', manager.last_name) as manager_name
from employees xodim inner join employees manager on xodim.manager_id = manager.employee_id;

--union:
--1-topshiriq:

select concat(first_name, ' ', last_name) as full_name from students
where first_name like 'A%' or first_name like 'E%' or first_name like 'I%' or first_name like 'O%' or first_name like 'U%'
union
select concat(first_name, ' ', last_name) as full_name from teachers
where first_name like 'A%' or first_name like 'E%' or first_name like 'I%' or first_name like 'O%' or first_name like 'U%';

--2-topshiriq: 

select *, concat(first_name, ' ', last_name) as full_name from students
union
select *, concat(first_name, ' ', last_name) as full_name from teachers
order by length(last_name), last_name;

--3-topshiriq: 

select *, concat(first_name, ' ', last_name) as full_name, 'student' as manba from students
union
select *, concat(first_name, ' ', last_name) as full_name, 'teacher' as manba from teachers
order by manba, last_name;

--4-topshiriq: 

select concat(first_name, ' ', last_name) as full_name, id from students
union
select concat(first_name, ' ', last_name) as full_name, id from teachers
order by id desc limit 3;

--intersect
--5-topshiriq: 

select product, quantity from orders
intersect select product, quantity from shipments;

--6-topshiriq: 

select product, quantity as order_quantity from orders 
intersect select product, quantity from shipments where quantity >= 5;

--7-topshiriq: 

select order_id, product, quantity from orders 
intersect select shipment_id, product, quantity from shipments;

--8-topshiriq: 

select shipment_id, product, quantity from shipments
intersect select order_id, product, quantity from orders;

--except 
--9-topshiriq: 
select product from orders except select product from shipments;

--10-topshiriq: 
select product from shipments except select product from orders order by product;

--11-topshiriq: 
select * from orders where quantity >= 10 except select * from shipments;

--12-topshiriq: 
select * from shipments where quantity < 5 except select * from orders;]]></console>
